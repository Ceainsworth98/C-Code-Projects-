#include <iostream>
using namespace std;

int main()
{
    //addition of two numbers

    int numberone, numbertwo,sum;
    numberone=150;
    numbertwo=450;

    sum= numberone + numbertwo;

    cout<<"The sum of "<<numberone<<" and "<<numbertwo<<" is "<<sum<<".";
    return 0;
}
